{
  "name": "promise.retry",
  "version": "2.0.3",
  "description": "add (timeout and fail) retry for async functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "gen-readme": "swig render ./_readme/readme.md > README.md && prettier --write README.md",
    "prepare": "husky",
    "tsd": "tsd",
    "test": "vitest run",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "typeswrong": "attw --pack ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicdawn/promise.retry.git"
  },
  "keywords": [
    "promise",
    "retry",
    "async"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/promise.retry/issues"
  },
  "homepage": "https://github.com/magicdawn/promise.retry#readme",
  "dependencies": {
    "promise.timeout": "^1.2.0"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^0.1.0",
    "@magicdawn/prettier-config": "^0.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "should": "^13.2.3",
    "swig-templates": "^2.0.3",
    "tsd": "^0.31.1",
    "vitest": "^1.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,less,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
